    <!-- JavaScript -->
    <script>
        // Pricing Calculator
        class BrochureCalculator {
            constructor() {
                this.basePrice = 46; // Base price per brochure in INR
                this.quantity = 250;
                this.size = 'A4';
                this.fold = 'tri-fold';
                this.paper = '130gsm';
                this.coating = 'gloss';
                this.delivery = 'standard';
                
                this.multipliers = {
                    size: { 'A4': 1.0, 'A5': 0.8, 'DL': 0.7, 'custom': 1.3 },
                    fold: { 'tri-fold': 1.0, 'z-fold': 1.1, 'bi-fold': 0.9 },
                    paper: { '130gsm': 1.0, '170gsm': 1.2, '250gsm': 1.5, '300gsm': 1.8 },
                    coating: { 'gloss': 1.1, 'matte': 1.1, 'uncoated': 1.0 },
                    delivery: { 'standard': 0, 'express': 500, 'rush': 1200 }
                };
                
                this.calculatePrice();
            }
            
            calculatePrice() {
                let unitPrice = this.basePrice;
                
                // Apply multipliers
                unitPrice *= this.multipliers.size[this.size];
                unitPrice *= this.multipliers.fold[this.fold];
                unitPrice *= this.multipliers.paper[this.paper];
                unitPrice *= this.multipliers.coating[this.coating];
                
                // Volume discounts
                if (this.quantity >= 5000) unitPrice *= 0.75;
                else if (this.quantity >= 2500) unitPrice *= 0.80;
                else if (this.quantity >= 1000) unitPrice *= 0.85;
                else if (this.quantity >= 500) unitPrice *= 0.90;
                
                const totalPrice = Math.round(unitPrice * this.quantity) + this.multipliers.delivery[this.delivery];
                const originalPrice = Math.round(totalPrice / 0.75); // 25% discount
                
                // Update display
                document.getElementById('finalPrice').textContent = '‚Çπ' + totalPrice.toLocaleString();
                document.getElementById('originalPrice').textContent = '‚Çπ' + originalPrice.toLocaleString();
                document.getElementById('perPiecePrice').textContent = '‚Çπ' + Math.round(unitPrice) + ' per brochure';
            }
        }

        const calculator = new BrochureCalculator();

        function updatePrice() {
            calculator.quantity = parseInt(document.getElementById('quantity').value);
            calculator.paper = document.getElementById('paperType').value;
            calculator.coating = document.getElementById('coating').value;
            calculator.calculatePrice();
        }

        function selectSize(element) {
            document.querySelectorAll('.size-option').forEach(btn => {
                btn.classList.remove('active', 'border-primary', 'bg-primary/10');
                btn.classList.add('border-gray-200');
                btn.querySelector('div').classList.remove('text-primary');
                btn.querySelector('div').classList.add('text-gray-700');
            });
            
            element.classList.add('active', 'border-primary', 'bg-primary/10');
            element.classList.remove('border-gray-200');
            element.querySelector('div').classList.add('text-primary');
            element.querySelector('div').classList.remove('text-gray-700');
            
            calculator.size = element.dataset.size;
            calculator.calculatePrice();
        }

        function selectFold(element) {
            document.querySelectorAll('.fold-option').forEach(btn => {
                btn.classList.remove('active', 'border-primary', 'bg-primary/10');
                btn.classList.add('border-gray-200');
                btn.querySelector('span').classList.remove('text-primary');
                btn.querySelector('span').classList.add('text-gray-700');
            });
            
            element.classList.add('active', 'border-primary', 'bg-primary/10');
            element.classList.remove('border-gray-200');
            element.querySelector('span').classList.add('text-primary');
            element.querySelector('span').classList.remove('text-gray-700');
            
            calculator.fold = element.dataset.fold;
            calculator.calculatePrice();
        }

        function selectDelivery(element) {
            document.querySelectorAll('.delivery-option').forEach(btn => {
                btn.classList.remove('active', 'border-primary', 'bg-primary/10');
                btn.classList.add('border-gray-200');
                btn.querySelector('div').classList.remove('text-primary');
                btn.querySelector('div').classList.add('text-gray-700');
            });
            
            element.classList.add('active', 'border-primary', 'bg-primary/10');
            element.classList.remove('border-gray-200');
            element.querySelector('div').classList.add('text-primary');
            element.querySelector('div').classList.remove('text-gray-700');
            
            calculator.delivery = element.dataset.speed;
            calculator.calculatePrice();
        }

        // Accordion functionality
        function toggleAccordion(index) {
            const content = document.getElementById(`accordion-${index}`);
            const icon = document.querySelectorAll('.accordion-icon')[index];
            
            if (content.style.maxHeight && content.style.maxHeight !== '0px') {
                content.style.maxHeight = '0px';
                icon.style.transform = 'rotate(0deg)';
            } else {
                // Close all other accordions
                document.querySelectorAll('.accordion-content').forEach((item, i) => {
                    if (i !== index) {
                        item.style.maxHeight = '0px';
                        document.querySelectorAll('.accordion-icon')[i].style.transform = 'rotate(0deg)';
                    }
                });
                
                content.style.maxHeight = content.scrollHeight + 'px';
                icon.style.transform = 'rotate(180deg)';
            }
        }

        // FAQ functionality
        function toggleFAQ(index) {
            const content = document.getElementById(`faq-${index}`);
            const icon = document.querySelectorAll('.faq-icon')[index];
            
            if (content.style.maxHeight && content.style.maxHeight !== '0px') {
                content.style.maxHeight = '0px';
                icon.style.transform = 'rotate(0deg)';
            } else {
                content.style.maxHeight = content.scrollHeight + 'px';
                icon.style.transform = 'rotate(180deg)';
            }
        }

        // Reviews carousel
        let currentReview = 0;
        const totalReviews = 4;
        const reviewsPerView = window.innerWidth >= 1024 ? 3 : (window.innerWidth >= 768 ? 2 : 1);

        function moveCarousel(direction) {
            const track = document.getElementById('reviewsTrack');
            currentReview += direction;
            
            if (currentReview < 0) currentReview = totalReviews - reviewsPerView;
            if (currentReview > totalReviews - reviewsPerView) currentReview = 0;
            
            const translateX = -(currentReview * (100 / reviewsPerView));
            track.style.transform = `translateX(${translateX}%)`;
        }

        // Auto-play carousel
        setInterval(() => {
            moveCarousel(1);
        }, 5000);

        // File upload functionality
        document.getElementById('fileInput').addEventListener('change', function(e) {
            const files = e.target.files;
            if (files.length > 0) {
                const fileNames = Array.from(files).map(file => file.name).join(', ');
                const uploadArea = e.target.parentElement;
                uploadArea.innerHTML = `
                    <svg class="w-8 h-8 text-green-500 mx-auto mb-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"></path>
                    </svg>
                    <p class="text-green-600 font-semibold">Files uploaded successfully!</p>
                    <p class="text-sm text-gray-500">${fileNames}</p>
                `;
            }
        });

        // Mobile menu toggle
        document.getElementById('mobile-menu-btn').addEventListener('click', function() {
            // Add mobile menu functionality here
        });

        // Initialize on page load
        document.addEventListener('DOMContentLoaded', function() {
            calculator.calculatePrice();
        });
    </script>
